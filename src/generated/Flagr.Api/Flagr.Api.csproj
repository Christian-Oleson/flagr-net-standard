<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard1.3</TargetFramework>
    <AssemblyName>Flagr.Api</AssemblyName>
    <PackageId>Flagr.Api</PackageId>
    <OutputType>Library</OutputType>
    <Authors>Swagger</Authors>
    <Company>Swagger</Company>
    <AssemblyTitle>Swagger Library</AssemblyTitle>
    <Description>A library generated from a Swagger doc</Description>
    <Copyright>No Copyright</Copyright>
    <GenerateRuntimeConfigurationFiles>true</GenerateRuntimeConfigurationFiles>
    <AutoGenerateBindingRedirects>true</AutoGenerateBindingRedirects>
    <GenerateBindingRedirectsOutputType>true</GenerateBindingRedirectsOutputType>
    <RootNamespace>Flagr.Api</RootNamespace>
    <Version>1.0.0</Version>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="12.0.3" />
    <PackageReference Include="JsonSubTypes" Version="1.8.0" />
  </ItemGroup>


</Project>